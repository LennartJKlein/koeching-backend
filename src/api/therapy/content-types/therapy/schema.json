{
  "kind": "collectionType",
  "collectionName": "therapies",
  "info": {
    "singularName": "therapy",
    "pluralName": "therapies",
    "displayName": "Therapie",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "coaches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::coach.coach",
      "inversedBy": "therapies"
    },
    "interventions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::intervention.intervention",
      "mappedBy": "therapies"
    },
    "content": {
      "type": "richtext"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "seo_title": {
      "type": "string"
    },
    "seo_description": {
      "type": "string"
    },
    "intro": {
      "type": "text"
    },
    "pricing": {
      "type": "component",
      "repeatable": true,
      "component": "details.price"
    },
    "location": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "seo_keywords": {
      "type": "component",
      "repeatable": true,
      "component": "details.keywords",
      "max": 5
    },
    "rank": {
      "type": "integer"
    }
  }
}
